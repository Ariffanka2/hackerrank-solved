import math

import os import random

import re

import sys

# Complete the 'simpleArraySum' function below.

#

# The function is expected to return an INTEGER. # The function accepts INTEGER_ARRAY ar as parameter. #

x=0

def simpleArraySum(ar): for i in range (0,ar_count): x= x + ar[i] # Write your code here

return x

if __name__ == '__main__': fptr = open (os.environ['OUTPUT_PATH'], 'w')

ar_count = int(input().strip()) ar = list (map (int, input().rstrip ().split()))

result = simpleArraySum (ar)

fptr.write(str (result) + '\n')

fptr.close()
